<html>
<body>
    <h1>Cadastro de Usuario</h1>
    <form action="/cadastro/usuario/salvar" method="post">
        <label>Nome:</label>
        <input name="nome" type="text"> <br>
        <label>Senha:</label>
        <input name="senha" type="password"> <br>
        <input type="submit" value="Entrar">
    </form>
    <% if(messagemLogin) {%>
        <p>
            <label style="color: red"> <%= mensagemLogin %> </label>
        </p>
        <%}%}>
</body>
</html>